package exam_02_04_BookShelf;

import java.util.Date;

public class TimeCheck {

	private long time;
	private long start;
	private long end;
	
	public void startTime()
	{
		Date today1 = new Date();
		start = today1.getTime();
		
	}
	
	public void endTime()
	{
		Date today1 = new Date();
		end = today1.getTime();
		
	}
	
	public void printTIme()
	{
		System.out.println((end-start) + "초 걸렸습니다.!!");
	}
}
	


